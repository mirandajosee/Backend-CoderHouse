<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
<img src="/mirai.png" alt="Logo de Mirai Ecommerce" width="20%" height="20%"/>
<body class="container" style="background-color:rgb(170, 142, 228);color:honeydew">
    <h1 style="padding-block:2%">Registro</h1>
    <form action="/api/users/register" method="post" class="form-group w-50" style="padding-bottom:1%">
        <label class="form-label">Nombre</label>
        <input   class="form-control" name="firstName">
        <label class="form-label">Apellido</label>
        <input type="text" class="form-control" name="lastName">
        <label class="form-label">Email</label>
        <input type="text" class="form-control" name="email">
        <label class="form-label">Edad</label>
        <input type="text" class="form-control" name="age">
        <label class="form-label">Contraseña</label>
        <input type="password" class="form-control" name="password">
        <input type="submit">
    </form>
    <p>¿Ya tenes cuenta? <a href="/login">Ingresa aquí</a></p>
    <form action="/api/users/githubcallback" method="get" class="form-group w-50">
    <input type="submit" value="Registrarse con GitHub">
    </form>
</body>
